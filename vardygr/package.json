{
  "name": "vardygr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "deploy": "react-native bundle --entry-file ./index.ios.js --platform ios --bundle-output ios/main.jsbundle && fastlane beta",
    "ios": "open ios/RNApp.xcodeproj",
    "lint": "eslint . --fix",
    "shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82"
  },
  "dependencies": {
    "react": "15.2.1",
    "react-native": "0.30.0",
    "react-native-background-geolocation": "^1.0.1",
    "react-native-background-geolocation-android": "git+ssh://git@github.com/transistorsoft/react-native-background-geolocation-android.git#2.0.0",
    "react-native-fbsdk": "^0.2.1",
    "react-native-google-places-autocomplete": "^1.2.0",
    "react-native-maps": "^0.7.1",
    "rn-splash-screen": "^2.0.0",
    "@exponent/react-native-navigator": "^0.4.7",
    "gravatar-api": "^1.3.4",
    "react-native-keyboard-spacer": "^0.3.0",
    "react-native-meteor": "1.0.0-rc14",
    "react-native-tab-navigator": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.1.1"
  }
}